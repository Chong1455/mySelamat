    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.cks.my_selamat2",
      "versionCode": 2,
      "permissions": ["CAMERA"]
    }
  }
}
