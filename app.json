{
  "expo": {
    "name": "MySelamat",
    "slug": "my-selamat",
    "privacy": "public",
    "sdkVersion": "42.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/MySelamat.png",
    "splash": {
      "image": "./assets/MySelamat.png",
